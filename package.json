{
  "name": "solid-obsidian",
  "version": "0.6.1",
  "description": "An unofficial SolidJS port of Obsidian UI components.",
  "license": "MIT",
  "author": "Ryota Ushio",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyotaUshio/solid-obsidian.git"
  },
  "homepage": "https://github.com/RyotaUshio/solid-obsidian#readme",
  "bugs": {
    "url": "https://github.com/RyotaUshio/solid-obsidian/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      }
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {},
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "test": "concurrently pnpm:test:*",
    "test:client": "vitest",
    "test:ssr": "pnpm run test:client --mode ssr",
    "prepublishOnly": "pnpm build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "lint": "concurrently pnpm:lint:*",
    "lint:code": "eslint --max-warnings 0 --fix",
    "lint:types": "tsc --noEmit",
    "update-deps": "pnpm up -Li"
  },
  "peerDependencies": {
    "solid-js": "^1.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^24.0.10",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "concurrently": "^9.2.0",
    "esbuild": "^0.25.5",
    "esbuild-plugin-solid": "^0.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-solid": "^0.14.5",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "obsidian": "^1.8.7",
    "prettier": "3.6.2",
    "solid-js": "^1.9.7",
    "tsup": "^8.5.0",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^6.3.5",
    "vite-plugin-solid": "^2.11.7",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "solid",
    "obsidian"
  ],
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "node": ">=18",
    "pnpm": ">=10.11.0"
  }
}
